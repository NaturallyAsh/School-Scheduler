<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NewWeekView">
        <attr format="enum" name="newFirstDayOfWeek">
            <enum name="sunday" value="1"/>
            <enum name="monday" value="2"/>
            <enum name="tuesday" value="3"/>
            <enum name="wednesday" value="4"/>
            <enum name="thursday" value="5"/>
            <enum name="friday" value="6"/>
            <enum name="saturday" value="7"/>
        </attr>
        <attr name="allDayEventHeight" format="dimension"/>
        <attr name="autoLimitTime" format="boolean"/>
        <attr format="dimension" name="hourHeight"/>
        <attr format="dimension" name="minHourHeight"/>
        <attr format="dimension" name="maxHourHeight"/>
        <attr format="dimension" name="textSize"/>
        <attr format="dimension" name="eventTextSize"/>
        <attr format="dimension" name="headerColumnPadding"/>
        <attr format="dimension" name="headerRowPadding"/>
        <attr format="dimension" name="columnGap"/>
        <attr format="color" name="headerColumnTextColor"/>
        <attr format="integer" name="noOfVisibleDays"/>
        <attr format="color" name="headerRowBackgroundColor"/>
        <attr format="color" name="dayBackgroundColor"/>
        <attr format="color" name="hourSeparatorColor"/>
        <attr format="color" name="todayBackgroundColor"/>
        <attr format="color" name="todayHeaderTextColor"/>
        <attr format="dimension" name="hourSeparatorHeight"/>
        <attr format="color" name="eventTextColor"/>
        <attr format="dimension" name="eventPadding"/>
        <attr format="color" name="headerColumnBackground"/>
        <attr format="enum" name="newDayNameLength">
            <enum name="length_short" value="1"/>
            <enum name="length_long" value="2"/>
        </attr>
        <attr format="dimension" name="overlappingEventGap"/>
        <attr format="dimension" name="eventMarginVertical"/>
        <attr format="float" name="xScrollingSpeed"/>
        <attr format="dimension" name="eventCornerRadius"/>
        <attr format="boolean" name="showDistinctPastFutureColor"/>
        <attr format="boolean" name="showDistinctWeekendColor"/>
        <attr format="color" name="futureBackgroundColor"/>
        <attr format="color" name="pastBackgroundColor"/>
        <attr format="color" name="futureWeekendBackgroundColor"/>
        <attr format="color" name="pastWeekendBackgroundColor"/>
        <attr format="boolean" name="showNowLine"/>
        <attr format="color" name="nowLineColor"/>
        <attr format="dimension" name="nowLineThickness"/>
        <attr format="boolean" name="horizontalFlingEnabled"/>
        <attr format="boolean" name="verticalFlingEnabled"/>
        <attr name="maxTime" format="integer"/>
        <attr name="minTime" format="integer"/>
        <attr name="minOverlappingMinutes" format="integer"/>
        <attr name="newEventColor" format="color" />
        <attr name="newEventId" format="integer" />
        <attr name="newEventIdentifier" format="string" />
        <attr name="newEventIconResource" format="integer" />
        <attr name="newEventLengthInMinutes" format="integer" />
        <attr name="newEventTimeResolutionInMinutes" format="integer" />
        <attr name="scrollDuration" format="integer"/>
        <attr name="showFirstDayOfWeekFirst" format="boolean"/>
        <attr name="timeColumnResolution" format="integer"/>
        <attr name="zoomFocusPoint" format="fraction"/>
        <attr name="zoomFocusPointEnabled" format="boolean" />
        <attr name="dropListenerEnabled" format="boolean" />

    </declare-styleable>
</resources>